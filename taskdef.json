{
  "executionRoleArn": "arn:aws:iam::609946765885:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "deno-backend",
      "image": "deno-backend",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 4000,
          "protocol": "tcp",
          "containerPort": 4000
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "ecs-demo"
}
